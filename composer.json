{
    "name": "mileschou/phalgorithm",
    "license": "MIT",
    "description": "General Algorithm",
    "type": "library",
    "keywords": [
        "algorithm"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 | ^8.0",
        "brick/math": "^0.8.15"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.16 | ^1.0",
        "phpunit/phpunit": "^7 | ^8 | ^9",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^4 | ^5",
        "symfony/var-dumper": "^4 | ^5"
    },
    "autoload": {
        "psr-4": {
            "Phalgorithm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app",
            "Benchmarks\\": "benchmarks",
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
