{
    "name": "mileschou/phalgorithm",
    "license": "MIT",
    "description": "General Algorithm",
    "type": "library",
    "keywords": [
        "algorithm"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.10.0",
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Phalgorithm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benchmarks\\": "benchmarks",
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.5"
        }
    }
}
